// Test file for optional modifier syntax

class Person {
    // Mandatory fields (default [1])
    String firstName;
    String lastName;

    // Optional fields using optional modifier (equivalent to [0..1])
    String middleName[optional];
    String nickname[optional];
    Int age[optional];

    // Explicit syntax still works
    String email[0..1];

    // Arrays
    String phoneNumbers[0..*];
}
