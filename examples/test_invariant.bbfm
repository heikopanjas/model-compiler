// Test file for invariant constraints

class Image {
    feature width: Int;
    feature height: Int;
    feature format: String;

    invariant validWidth: width <= 3000;
    invariant validHeight: height <= 3000;
    invariant minWidth: width >= 100;
}

class Video {
    feature duration: Int;
    feature bitrate: Int;

    invariant maxDuration: duration <= 7200;
    invariant minBitrate: bitrate >= 128;
}
