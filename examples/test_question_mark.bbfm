// Test file for optional modifier syntax

class Person {
    // Mandatory fields (default [1])
    feature firstName: String;
    feature lastName: String;

    // Optional fields using optional modifier (equivalent to [0..1])
    feature middleName: String [optional];
    feature nickname: String [optional];
    feature age: Int [optional];

    // Explicit syntax still works
    feature email: String [0..1];

    // Arrays
    feature phoneNumbers: String [0..*];
}
