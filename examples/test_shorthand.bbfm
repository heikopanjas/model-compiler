// Test file for shorthand modifier syntax

class TestType {
    // New shorthand syntax - defaults to [1]
    feature name: String;
    feature age: Int;

    // Optional field using optional modifier (equivalent to [0..1])
    feature middleName: String [optional];

    // Explicit modifiers still work
    feature email: String [1,unique];
    feature nickname: String [0..1];        // Same as middleName[optional] above
    feature tags: String [0..*];
}
