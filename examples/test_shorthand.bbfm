// Test file for shorthand modifier syntax

class TestType {
    // New shorthand syntax - defaults to [1]
    String name;
    Int age;

    // Optional field using optional modifier (equivalent to [0..1])
    String middleName[optional];

    // Explicit modifiers still work
    String email[1,unique];
    String nickname[0..1];        // Same as middleName[optional] above
    String tags[0..*];
}
